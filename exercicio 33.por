programa {
  funcao inicio() {
    inteiro n1, n2
    real n3, ra, rb, rc
    escreva ("Informe o 1° número inteiro: ")
    leia (n1)
    escreva ("Informe o 2° número inteiro: ")
    leia (n2)
    escreva ("Informe o 3° número, sendo ele em casa decimal: ")
    leia (n3)
    ra = (n1 * n1) + (n2 / n2)
    escreva ("O resultado é: ", ra)
    rb = (n1 * n1 * n1) + n2
    escreva ("\nO resultado é: ", rb)
    rc = (n2 * n2) * n2
    escreva ("\nO resultado é: ", rc)
  }
}
